{
    "extends": "airbnb",
    "plugins": [
        "jsx-a11y",
        "import",
        "react"
    ],
    "rules": {
        "semi": 2,
        "import/no-extraneous-dependencies": [
          "error", {
              "devDependencies": true
          }
        ]
    },

    "globals": {
        "describe": true,
        "it": true,
        "expect": true,
        "app": true,
        "request": true,
        "beforeEach": true,
        "Sequelize": true,
        "td": true
    }
}