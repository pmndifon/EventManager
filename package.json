{
  "name": "eventmanager",
  "version": "1.0.0",
  "description": "\"Andela Fellowship Cycle 29 Pre-Bootcamp Developer Challenge - Event Manager App: This app will help to accept applications to use event center / facilities, and will either decline events when the proposed day is already taken, or suggest an available day.\",",
  "main": "server/server.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.23.4",
    "sequelize-cli": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.3",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.6"
  },
  "scripts": {
    "start": "babel-watch server/server.js",
    "test": "nyc --reporter=html --reporter=text mocha server/test/dummy/test.js --timeout 10000 --compilers js:babel-core/register --exit",
    "lint": "eslint '**/*.js' esw **/*.js config --color",
    "lint:fix": "eslint '**/*.js' --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "js": "nyc  --reporter=html --reporter=test mocha --compilers js:babel-register server/test/test.js",
    "test-integration": "mocha --opts server/test/integration/mocha.opts server/test/integration/*.js",
    "test-unit": "mocha --opts server/test/unit/mocha.opts server/test/unit/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndifon/EventManager.git"
  },
  "author": "Patrick Ndifon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pmndifon/EventManager/issues"
  },
  "homepage": "https://github.com/pmndifon/EventManager#readme"
}
